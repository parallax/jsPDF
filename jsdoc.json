{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "./",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|dist|examples)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash/",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
